.address-list-content {
  padding-top: 10px;
}
.address-list-content .address-list-title {
  width: 95%;
  margin: 0 auto;
  height: 30px;
}
.address-list-content .address-list-title .address-list-title-inner {
  font-size: 13px;
  color: red;
}
.address-list-content .address-list-title .address-list-title-inner .cancel {
  float: left;
  width: 48%;
}
.address-list-content .address-list-title .address-list-title-inner .manage,
.address-list-content .address-list-title .address-list-title-inner .complate {
  float: right;
}
.address-list-content .address-list-container {
  font-size: 14px;
  background-color: whitesmoke;
  padding-top: 5px;
}
.address-list-content .address-list-container .address-item {
  width: 100%;
  margin: 10px auto;
  background-color: white;
  border-radius: 10px;
  padding: 10px 0;
}
.address-list-content .address-list-container .address-item .item-inner {
  width: 95%;
  margin: 0 auto;
}
.address-list-content .address-list-container .address-item .item-inner .address-top .map-icon {
  display: inline-block;
}
.address-list-content .address-list-container .address-item .item-inner .address-top .item-text {
  display: inline-block;
  vertical-align: middle;
  width: 83%;
  margin: 0 1.5%;
}
.address-list-content .address-list-container .address-item .item-inner .address-top .item-text .text-name {
  font-weight: bolder;
  font-size: 15px;
}
.address-list-content .address-list-container .address-item .item-inner .address-top .item-edit {
  display: inline-block;
  vertical-align: middle;
  float: right;
  margin-top: 10px;
}
.address-list-content .address-list-container .address-item .item-inner .address-operate {
  border-top: 1px solid gray;
  margin-top: 5px;
  padding-top: 5px;
}
.address-list-content .address-list-container .address-item .item-inner .address-operate .set-default {
  display: inline-block;
}
.address-list-content .address-list-container .address-item .item-inner .address-operate .set-default .u-icon {
  display: inline-block;
}
.address-list-content .address-list-container .address-item .item-inner .address-operate .set-default ._span {
  display: inline-block;
  vertical-align: top;
}
.address-list-content .address-list-container .address-item .item-inner .address-operate .single-delete {
  display: inline-block;
  vertical-align: top;
  float: right;
}
.address-list-content .address-bottom {
  width: 100%;
  position: fixed;
  bottom: 0;
  background: white;
}
.address-list-content .address-bottom .u-button {
  width: 98%;
  margin: 0 auto;
}

