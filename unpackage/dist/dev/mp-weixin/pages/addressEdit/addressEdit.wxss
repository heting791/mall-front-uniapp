.address-edit-content {
  margin-top: 10px;
  background-color: whitesmoke;
}
.address-edit-content .address-edit-content-inner .inner-normal {
  background-color: white;
}
.address-edit-content .address-edit-content-inner .inner-normal .inner-normal-inner {
  width: 94%;
  margin: 0 auto;
}
.address-edit-content .address-edit-content-inner .inner-normal .inner-normal-inner .phone-prefix ._span {
  display: inline-block;
  margin-right: 3px;
}
.address-edit-content .address-edit-content-inner .inner-normal .inner-normal-inner .phone-prefix .u-icon {
  display: inline-block;
  vertical-align: top;
  margin-top: 2px;
}
.address-edit-content .address-edit-content-inner .inner-other {
  margin: 20px 0;
  background-color: white;
}
.address-edit-content .address-edit-content-inner .inner-other .other-tags {
  width: 94%;
  margin: 0 auto;
}
.address-edit-content .address-edit-content-inner .inner-other .other-tags .u-transition {
  margin-right: 15px;
}
.address-edit-content .address-edit-content-inner .inner-other .other-setdefault {
  width: 94%;
  margin: 0 auto;
}
.address-edit-content .address-edit-content-inner .inner-submit {
  position: fixed;
  bottom: 0;
  width: 100%;
  background-color: white;
}
.address-edit-content .address-edit-content-inner .inner-submit .u-button {
  width: 93%;
  margin: 0 auto;
}
.address-edit-content .address-edit-content-inner .inner-popup .phone-prefix-list .list {
  width: 650rpx;
}
.address-edit-content .address-edit-content-inner .inner-popup .phone-prefix-list .list ._ul {
  margin: 0;
  padding: 0;
  width: 90%;
  margin: 0 auto;
}
.address-edit-content .address-edit-content-inner .inner-popup .phone-prefix-list .list ._ul ._li {
  list-style-type: none;
  height: 40px;
  line-height: 40px;
}
.address-edit-content .address-edit-content-inner .inner-popup .phone-prefix-list .list ._ul ._li ._span {
  display: inline-block;
  width: calc(100% - 30px);
}
.address-edit-content .address-edit-content-inner .inner-popup .phone-prefix-list .list ._ul ._li .u-icon {
  display: inline-block;
  float: right;
  margin-top: 5px;
}
.address-edit-content .address-edit-content-inner .inner-popup .phone-prefix-list .list-bottom {
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  color: blue;
  border-top: 1px solid lightgray;
}

